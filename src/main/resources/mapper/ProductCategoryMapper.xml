<?xml version="1.0" encoding="UTF-8" ?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.sell.dataobject.mapper.ProductCategoryMapper">    <!--result映射到的dao层的数据对象，所以type对应的是到层的具体的数据对象-->    <resultMap id="BaseResultMap" type="com.sell.dataobject.ProductCategory">        <id column="category_id" property="categoryId" jdbcType="INTEGER"/>        <id column="category_name" property="categoryName" jdbcType="VARCHAR"/>        <id column="category_type" property="categoryType" jdbcType="INTEGER"/>    </resultMap>   <select id="selectByCategoryType" resultMap="BaseResultMap" parameterType="java.lang.Integer">      select category_id,category_name,category_type      from product_category      where category_type=#{category_type,jdbcType=INTEGER}    </select>    <select id="selectAll" resultMap="BaseResultMap">      select category_id,category_name,category_type      from product_category    </select></mapper>